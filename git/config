[core]
	editor = nvim
[alias]
	co = checkout
	ds = diff --staged
	st = status
	lg = log --all --graph --oneline
	add-edit-trailer = commit --amend --no-edit --trailer='edited-by: Micael Vignati <mvignati@ihmc.org>'
	add-co-authored-trailer = commit --amend --trailer='co-authored-by: Micael Vignati <mvignati@ihmc.org>'
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
